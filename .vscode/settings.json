{
	"files.autoSave": "afterDelay",
	"security.workspace.trust.untrustedFiles": "open",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.guides.bracketPairs": true,
	"editor.tabSize": 4,
	"[js]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.fontLigatures": true,
	"editor.fontSize": 14,
	"terminal.integrated.fontWeightBold": "bold",
	"terminal.integrated.gpuAcceleration": "auto",
	// "editor.fontFamily": "'JetBrainsMono Nerd Font Mono'",
	// "editor.fontFamily": "'FiraMono Nerd Font Mono'",
	// "editor.fontFamily": "'CaskaydiaMono Nerd Font Mono'",
	"editor.fontFamily": "'0xProto Nerd Font Mono'",
	"git.autofetch": true,
	"editor.formatOnSave": true,
	"explorer.confirmDelete": false,
	"git.openRepositoryInParentFolders": "never",
	"explorer.confirmDragAndDrop": false,
	"database-client.showUser": true,
	"database-client.showTrigger": true,
	"database-client.saveConnectionExpandedState": true,
	"window.confirmSaveUntitledWorkspace": false,
	"zenMode.centerLayout": true,
	"zenMode.restore": false,
	"editor.cursorBlinking": "expand",
	"terminal.integrated.cursorBlinking": true,
	"editor.largeFileOptimizations": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"git.enableSmartCommit": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
		"pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
		"*.dart": "${capture}.g.dart",
		"Cargo.toml": "Cargo.lock",
		"*.sqlite": "${capture}.${extname}-*",
		"*.db": "${capture}.${extname}-*",
		"*.sqlite3": "${capture}.${extname}-*",
		"*.db3": "${capture}.${extname}-*",
		"*.sdb": "${capture}.${extname}-*",
		"*.s3db": "${capture}.${extname}-*"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[rust]": {
		"editor.inlayHints.enabled": "offUnlessPressed",
	},
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.fontFamily": "FiraMono Nerd Font Mono",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.cursorStyleInactive": "underline",
	"terminal.external.osxExec": "Warp.app",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[python]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"better-comments.highlightPlainText": false,
	"better-comments.highlightComments": true,
	"better-comments.tags": [
		{
			"tag": "!",
			"color": "#E5E9C5",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "/",
			"color": "#00a5ab",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "hint:",
			"color": "#D4DF9E",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "?",
			"color": "#3498DB",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "todo:",
			"color": "#FFFFFF",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "*",
			"color": "#98C379",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "[incomplete]",
			"color": "#000000",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#eceff1",
			"bold": true,
			"italic": false
		}
	],
	"[html]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"workbench.activityBar.location": "top",
	"workbench.sideBar.location": "right",
	"terminal.integrated.defaultProfile.osx": "tmux",
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"git.ignoreRebaseWarning": true,
	"notebook.lineNumbers": "on",
	"notebook.formatOnSave.enabled": true,
	"redhat.telemetry.enabled": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"cmake.cmakePath": "/Applications/CMake.app/Contents/bin/cmake",
	"cmake.configureSettings": {
		"CMAKE_C_COMPILER": "/opt/homebrew/opt/llvm/bin/clang",
		"CMAKE_CXX_COMPILER": "/opt/homebrew/opt/llvm/bin/clang++"
	},
	"explorer.confirmPasteNative": false,
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"go.toolsManagement.autoUpdate": true,
	"github.copilot.enable": {
		"*": false,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
	},
	"makefile.configureOnOpen": true,
	"explorer.compactFolders": false,
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	"workbench.iconTheme": "file-icons",
	// "prettier.tabWidth": 4,
	// "prettier.useTabs": true,
	"terminal.integrated.fontLigatures.enabled": true,
	"workbench.colorCustomizations": {},
	"window.titleBarStyle": "custom",
	"python.analysis.typeCheckingMode": "standard",
	"docker.extension.enableComposeLanguageServer": false,
	"[dockercompose]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "advanced",
		"editor.quickSuggestions": {
			"other": true,
			"comments": false,
			"strings": true
		},
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"[github-actions-workflow]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"git.confirmSync": false,
	// all go Inlay Hints
	// "go.inlayHints.assignVariableTypes": true,
	// "go.inlayHints.compositeLiteralFields": true,
	// "go.inlayHints.compositeLiteralTypes": true,
	// "go.inlayHints.functionTypeParameters": true,
	// "go.inlayHints.parameterNames": true,
	// "go.inlayHints.rangeVariableTypes": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"comment",
					"emphasis",
					"keyword",
					"markup.italic",
					"storage.modifier"
					// Add any other specific scopes you find that are still italicized
				],
				"settings": {
					"fontStyle": ""
				}
			}
		]
	},
	"workbench.colorTheme": "Gruvbox Dark Hard",
	"geminicodeassist.project": "nomadic-system-88457",
	"geminicodeassist.displayInlineContextHint": false,
	"geminicodeassist.enable": true,
	"geminicodeassist.inlineSuggestions.enableAuto": false,
	"geminicodeassist.enableTelemetry": false,
	"diffEditor.codeLens": true,
	"C_Cpp.formatting": "clangFormat",
	"C_Cpp.intelliSenseUpdateDelay": 500,
	"C_Cpp.codeAnalysis.updateDelay": 500,
	"C_Cpp.clang_format_fallbackStyle": "llvm",
	"C_Cpp.intelliSenseEngine": "default",
	"C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
	"C_Cpp.inlayHints.parameterNames.enabled": true,
	"C_Cpp.inlayHints.referenceOperator.enabled": true,
	// You may also want to ensure the global setting is on
	"editor.inlayHints.enabled": "on",
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"geminicodeassist.agentYoloMode": true,
	"zig.zls.enabled": "on",
	"mesonbuild.downloadLanguageServer": true,
	"gitlens.ai.model": "gemini:gemini-2.5-flash",
	"gitlens.ai.vscode.model": "copilot:gpt-4.1",
	"terminal.integrated.profiles.osx": {
		"bash": {
			"path": "bash",
			"args": [
				"-l"
			],
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh",
			"args": [
				"-l"
			]
		},
		"fish": {
			"path": "fish",
			"args": [
				"-l"
			]
		},
		"tmux": {
			"path": "/opt/homebrew/bin/tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	"http.systemCertificatesNode": true,
	"editor.formatOnPaste": true,
	"cmake.showNotAllDocumentsSavedQuestion": false,
	"json.schemaDownload.trustedDomains": {
		"https://schemastore.azurewebsites.net/": true,
		"https://raw.githubusercontent.com/": true,
		"https://www.schemastore.org/": true,
		"https://json.schemastore.org/": true,
		"https://json-schema.org/": true,
		"https://biomejs.dev": true
	},
	"chat.agentsControl.enabled": false,
	"workbench.navigationControl.enabled": false,
	"window.commandCenter": false,
}
